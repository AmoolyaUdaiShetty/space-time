	Journal = {Journal of Virology},
	Language = {en},
	Month = may,
	Note = {{PMID:} 23468502},
	Number = {10},
	Pages = {5577--5585},
	Title = {The Source of the {PB1} Gene in Influenza Vaccine Reassortants Selectively Alters the Hemagglutinin Content of the Resulting Seed Virus},
