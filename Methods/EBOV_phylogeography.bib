	Number = {3},
	Pages = {1229--1236},
	Pmid = {12871927},
	Title = {Effect of {Recombination} on the {Accuracy} of the {Likelihood} {Method} for {Detecting} {Positive} {Selection} at {Amino} {Acid} {Sites}},
	Url = {http://www.genetics.org/content/164/3/1229},
	Urldate = {2015-01-12},
	Volume = {164},
