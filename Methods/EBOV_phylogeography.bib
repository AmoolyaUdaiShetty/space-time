	Journal = {Molecular biology and evolution},
	Month = apr,
	Number = {4},
	Pages = {817--827},
	Pmcid = {PMC1949848},
	Pmid = {16452117},
	Title = {Recombination Estimation Under Complex Evolutionary Models with the Coalescent Composite-Likelihood Method},
	Url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC1949848/},
