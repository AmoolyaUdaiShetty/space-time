	Title = {Characterization of a {Novel} {Influenza} {Virus} in {Cattle} and {Swine}: {Proposal} for a {New} {Genus} in the {Orthomyxoviridae} {Family}},
	Url = {http://mbio.asm.org/content/5/2/e00031-14},
