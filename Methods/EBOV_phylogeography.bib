	Issn = {0022-2844, 1432-1432},
	Journal = {Journal of Molecular Evolution},
	Keywords = {Approximate methods, Cell Biology, maximum likelihood, Microbiology, Phylogeny, Plant Sciences, Rate variation over sites, The gamma distribution},
	Language = {en},
