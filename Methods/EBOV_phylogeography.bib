	Doi = {10.1073/pnas.172393399},
	File = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/tit72ymn.default/zotero/storage/NQVUTZ6K/Hoffmann et al. - 2002 - Rescue of influenza B virus from eight plasmids.pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/tit72ymn.default/zotero/storage/HVPX9SNA/11411.html:text/html},
	Issn = {0027-8424, 1091-6490},
	Journal = {Proceedings of the National Academy of Sciences},
	Language = {en},
