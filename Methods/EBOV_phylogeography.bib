	Issn = {0014-3820},
	Journal = {Evolution},
	Month = jul,
	Number = {4},
	Pages = {783--791},
	Shorttitle = {Confidence {Limits} on {Phylogenies}},
	Title = {Confidence {Limits} on {Phylogenies}: {An} {Approach} {Using} the {Bootstrap}},
	Url = {http://www.jstor.org/stable/2408678},
