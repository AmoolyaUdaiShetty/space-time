	Pmid = {25031349},
	Title = {Rooting the Phylogenetic Tree of {M}iddle {E}ast {R}espiratory {S}yndrome {C}oronavirus by Characterization of a Conspecific Virus from an {A}frican Bat},
	Url = {http://jvi.asm.org/content/88/19/11297},
