	Month = may,
	Number = {5},
	Pages = {e10434},
	Title = {Guidelines for {Identifying} {Homologous} {Recombination} {Events} in {Influenza} {A} {Virus}},
	Url = {http://dx.doi.org/10.1371/journal.pone.0010434},
