@article{pourrut_2005,
	Abstract = {Several countries spanning the equatorial forest regions of Africa have had outbreaks of Ebola hemorrhagic fever over the last three decades. This article is an overview of the many published investigations of how Ebola virus circulates in its natural environment, focusing on the viral reservoir, susceptible animal species, environmental conditions favoring inter-species transmission, and how the infection is transmitted to humans. Major breakthroughs have been made in recent years but many outstanding questions must be dealt with if we are to prevent human outbreaks by interfering with the viral life cycle.},
	Author = {Pourrut, Xavier and Kumulungui, Brice and Wittmann, Tatiana and Moussavou, Ghislain and D{\'e}licat, Andr{\'e} and Yaba, Philippe and Nkoghe, Dieudonn{\'e} and Gonzalez, Jean-Paul and Leroy, Eric Maurice},
	Date-Added = {2015-05-18 15:25:52 +0100},
