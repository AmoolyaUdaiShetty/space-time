	Journal = {{PLoS} Currents},
	Month = sep,
	Pmcid = {PMC2762761},
	Pmid = {20029613},
	Title = {Reconstructing the initial global spread of a human influenza pandemic},
	Url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2762761/},
	Urldate = {2015-01-07},
