	Issn = {0042-6822},
	Journal = {Virology},
	Month = feb,
	Number = {2},
	Pages = {342--354},
	Shorttitle = {Analysis of intracellular small {RNAs} of mouse hepatitis virus},
	Title = {Analysis of intracellular small {RNAs} of mouse hepatitis virus: evidence for discontinuous transcription},
