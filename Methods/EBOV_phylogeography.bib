	Doi = {10.1002/jmv.10418},
	File = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/tit72ymn.default/zotero/storage/3UUF3PCI/Ansaldi et al. - 2003 - Molecular characterization of influenza B viruses .pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/tit72ymn.default/zotero/storage/G3HGFNSP/abstract.html:text/html},
